@{
    ViewBag.Title = "ProjectDetail";
}
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.projectname ProjectDetail</title>
    <link href="~/Content/css/reset.css" rel="stylesheet" />
    <link href="~/Content/css/projectdetail.css" rel="stylesheet" />
</head>
<body> 
@RenderPage("~/Views/Navigation/Navigation.cshtml")
<div id="wrapper">
<h2>@ViewBag.projectname ProjectDetail</h2>

<div id="listDesigns">
@foreach (var item in ViewBag.projectdetail) {
    <a class="action" href="@Url.Action("VersionDetail", "ProjectDetail", new { versionid=@item.versionid})">
<div class="listItem">
<p>@item.description</p>
<img src="~/uploads/@item.path" />
</div>
        </a>
}

        </div>
    </div>
</body>
</html>
